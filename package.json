{
	"name": "number-converter",
	"displayName": "Number Converter",
	"description": "Support increment, decrement, and Convert dec, hex and bin.",
	"publisher": "kamiyaowl",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.convert",
		"onCommand:extension.increment",
		"onCommand:extension.decrement"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.convert",
				"title": "Number Converter: Convert Number"
			},
			{
				"command": "extension.increment",
				"title": "Number Converter: Increment"
			},
			{
				"command": "extension.decrement",
				"title": "Number Converter: Decrement"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	}
}
